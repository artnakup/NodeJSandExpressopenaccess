<!DOCTYPE html>
<html lang="en" class="scroll-smooth">

<head>

</head>

<body>
  <h1>PRODUCTS</h1>
  <% products.map((product) => { %>
    <h4>
      <a href="/products/<%= product.id %>">
        <%= product.productTitle %>
      </a>
    </h4>
    <p>
      <%= product.productDescription %>
    </p>
    <p>
      <%= product.productPrice %>
    </p>
  <% }) %>

</body>

</html>
